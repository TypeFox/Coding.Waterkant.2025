        "langium:generate": "npm run langium:generate --workspace=packages/semiformal-web-apps",
        "generator-ai:build": "npm run build --workspace=packages/generator-ai"
    },
    "devDependencies": {
        "rimraf": "^6.0.1"
    },
    "keywords": [
        "Langium",
        "Semiformal",
        "AI"
